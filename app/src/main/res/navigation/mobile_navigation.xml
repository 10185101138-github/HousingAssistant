<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_shelter">

    <fragment
        android:id="@+id/navigation_shelter"
        android:name="com.aresix.housingassistant2.ui.home.HomeFragment"
        android:label="@string/title_shelter"
        tools:layout="@layout/fragment_shelter" />

    <fragment
        android:id="@+id/navigation_food"
        android:name="com.aresix.housingassistant2.ui.dashboard.DashboardFragment"
        android:label="@string/title_food"
        tools:layout="@layout/fragment_food" />

    <fragment
        android:id="@+id/navigation_transportation"
        android:name="com.aresix.housingassistant2.ui.notifications.NotificationsFragment"
        android:label="@string/title_transportation"
        tools:layout="@layout/fragment_transportation" />

    <fragment
        android:id="@+id/navigation_clothing"
        android:name="com.aresix.housingassistant2.ui.notifications.NotificationsFragment"
        android:label="@string/title_clothing"
        tools:layout="@layout/fragment_clothing" />
